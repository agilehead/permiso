{
  "name": "@agilehead/permiso-client",
  "version": "0.0.6",
  "description": "Client library for Permiso RBAC API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
