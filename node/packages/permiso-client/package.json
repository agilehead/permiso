    "test:grep": "mocha --exit --grep",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@codespin/permiso-test-utils": "file:../permiso-test-utils",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "chai": "^5.2.1",
    "mocha": "^11.7.1",
    "pg": "^8.16.3",
    "tsx": "^4.19.4",
    "typescript": "^5.7.3"
  },
  "keywords": ["rbac", "permissions", "access-control", "client", "api"],
  "author": "Agilehead",
  "license": "MIT"
